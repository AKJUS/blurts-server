{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true,
        "jest": true
    },
    "extends": [
        "standard"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "no-prototype-builtins": "warn",
        "no-undef": "warn",
        "camelcase": "warn",
        "new-cap": "warn"
    }
}
